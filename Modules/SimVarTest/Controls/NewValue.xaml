<UserControl x:Class="Eng.Chlaot.Modules.SimVarTestModule.Controls.NewValue"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Eng.Chlaot.Modules.SimVarTestModule.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="32" d:DesignWidth="800">
  <StackPanel Orientation="Horizontal">
    <TextBox x:Name="txtValue" Width="100" />
    <Button Content="Apply" x:Name="btnApply" Click="btnApply_Click"/>
  </StackPanel>
</UserControl>

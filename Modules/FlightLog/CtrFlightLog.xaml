<UserControl x:Class="Eng.EFsExtensions.Modules.FlightLogModule.CtrFlightLog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Eng.EFsExtensions.Modules.FlightLogModule"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
  <TabControl>
    <TabItem Header="Log">
      <DataGrid ItemsSource="{Binding Flights}" AutoGenerateColumns="False">
      </DataGrid>
    </TabItem>
  </TabControl>
</UserControl>
